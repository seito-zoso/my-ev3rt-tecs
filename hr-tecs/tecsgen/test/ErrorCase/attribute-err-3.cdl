import_C( "cygwin_tecs.h" );

namespace nNS2{
  [active]
    celltype tCelltype {
    attr {
      [size_is(2)]				// size_is があれば問題ない
        int     *a = { 1, 2 };
      char	*b = "abc";
    };
    var {
      [size_is(2)]				// size_is があれば問題ない
        int     *c = { 1, 2 };
    };
  };
};

region rR2 {
  cell nNS2::tCelltype Cell1 {
  };
  cell nNS2::tCelltype Cell2 {
    a = { 3, 4 };
  };
};


