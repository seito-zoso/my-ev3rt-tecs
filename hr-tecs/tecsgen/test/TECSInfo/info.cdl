celltype tTaskMain {
    entry sTaskBody            eBody;
    call  nTECSInfo::sTECSInfo cTECSInfo;
};

cell tTask Task {
    taskAttribute = C_EXP("TA_ACT");
    priority      = 11;
    stackSize     = 4096;
    cBody         = TaskMain.eBody;
};

cell tTaskMain TaskMain {
    cTECSInfo  = TECSInfo.eTECSInfo;
};

